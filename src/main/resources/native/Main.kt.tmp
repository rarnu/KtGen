import kotlinx.coroutines.CoroutineScope
import kotlin.coroutines.CoroutineContext
import kotlin.coroutines.EmptyCoroutineContext

expect val osName: String
expect val exePath: String
expect fun <T> coroutine (context: CoroutineContext = EmptyCoroutineContext, block: suspend CoroutineScope.() -> T): T

fun main(args: Array<String>) = coroutine {
    println("OsName: $osName")
    println("Execute: $exePath")
}
